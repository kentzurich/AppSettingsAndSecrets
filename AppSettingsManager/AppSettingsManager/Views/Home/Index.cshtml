@inject Microsoft.Extensions.Configuration.IConfiguration config;
@inject Microsoft.Extensions.Options.IOptions<SocialLoginSettings> socialLoginSettings;

<div class="row mb-2">
    <div class="col-md-6">
        <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
                <strong class="d-inline-block mb-2 text-primary">SendGrid</strong>
                <div class="mb-1 text-muted">@ViewBag.SendGridKey</div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
                <strong class="d-inline-block mb-2 text-primary">3rd Level Setting</strong>
                <div class="mb-1 text-muted">@ViewBag.ThirdLevelSetting</div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
                <strong class="d-inline-block mb-2 text-primary">Twilio from Controller</strong>
                <div class="mb-1 text-muted">@ViewBag.TwilioAuthToken</div>
                <div class="mb-1 text-muted">@ViewBag.TwilioAccountSid</div>
                <div class="mb-1 text-muted">@ViewBag.TwilioPhoneNumber</div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
                <strong class="d-inline-block mb-2 text-primary">Twilio Injected to View</strong>
                <div class="mb-1 text-muted">@config.GetSection("Twilio:AuthToken").Value</div>
                <div class="mb-1 text-muted">@config.GetSection("Twilio:AccountSid").Value</div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
                <strong class="d-inline-block mb-2 text-primary">Social Login From Controller</strong>
                <div class="mb-1 text-muted">@ViewBag.FacebookKey</div>
                <div class="mb-1 text-muted">@ViewBag.GoogleKey</div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
                <strong class="d-inline-block mb-2 text-primary">Social Login From View</strong>
                <div class="mb-1 text-muted">@socialLoginSettings.Value.FacebookSettings.Key</div>
                <div class="mb-1 text-muted">@socialLoginSettings.Value.GoogleSettings.Key</div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
                <strong class="d-inline-block mb-2 text-primary">ConnectionString</strong>
                <div class="mb-1 text-muted">@ViewBag.ConnectionString</div>
            </div>
        </div>
    </div>
</div>